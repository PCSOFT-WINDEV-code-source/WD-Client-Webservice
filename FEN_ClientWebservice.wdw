   -
     internal_properties : CAAAAAgAAAALrfDjEyDyMXWRIn8oy4BZ5uwBXFHxyO3sbnEf0i9krbe5cgse5JXM6BULlgKtYXxyknrEJvkn/PEKcET2SsP0+hjqqbp8bTa2ADxB6/7TqqbL/qKN45TVvdK8wN1TehjS3I8AD7VMJ7rknFCiIlliecuulLmanomZ2FKUvSODCwX5FWS1Wd5honVU8qqzNhEzceMc7cKjWB4M7EWyvmz/
     code : |1-
      
      CONSTANTE CST_NOM_WDL = "WD Serveur Webservice"
      
      GLOBAL
      	gfnTableauResultat est un tableau dynamique		// Tableau contenant le résultat de la recherche
      	gfnIndiceResultat est un entier					// Indice en cours dans le résultat de la recherche
      	gfsFicINI est une chaine
      LOCALE
      	
      gfnIndiceResultat = 1
      
      // Fichier ".ini" de configuration
      gfsFicINI = fRepExe()+"\"+ProjetInfo(piNomProjet)+".ini"
      Si pas fFichierExiste(gfsFicINI) ALORS
      	fCrée(gfsFicINI)
      FIN
      
      GROUPE_PARCOURS..Visible = Faux
