  procedures :
   -
     name : AfficherResultat
     internal_properties : CAAAAAgAAABu30G5B9IkQd6Cvxh/GRs++JFGxeqHGbxVS9pE0MHwF1azcXHnWwGUGda7+egABpm8GxVQWjTFtCPDMLqz0n0ARVM34KSKM7Zz/ALtz8LL4AcUT9JJjFvA4gx5Gmf6e7q/7KYtjgMawR7iSlG1
     procedure_id : 2087280253894421299
     type_code : 14
     code : |1+
      Procédure AfficherResultat()
      
      // On Affiche l'image
      Image_Resultat = fRepexe()+"\Temp.jpg"
     type : 458752
   -
     name : bRecupererImage
     internal_properties : CAAAAAgAAABO/2GYLxIGserozYQjqY9Y8Jum99JJJagN7bRg/PmI3YJ52aPTncWQFcTlRVxQDtd4SSXS2gpm2QTin8tOaWRvEuCEEXetmJdAg6Feuini50yRDJdQE6z9D+9UA8qZSoHkKUdWMb6FWv8CanGV
     procedure_id : 2087280253894487007
     type_code : 14
     code : |1-
      Procédure bRecupererImage(nNumImage)
      
      sResultat est une chaine
      nIdFichier est un entier
      
      
      // Appel au web service
      sResultat =  Décrypte(WD_Serveur_Webservice.RenvoyerImage(nNumImage), "",crypteAucun,encodeBASE64)
      
      si sResultat~="" alors
      	Erreur(<§$0008§>,ErreurInfo())
      	RENVOYER Faux
      FIN
      
      // Création du fichier de visualisation 
      nIdFichier = fouvre(fRepexe()+"\Temp.jpg",foCréation)
      si nIdFichier<>-1 alors
      	// On reconstruit le fichier
      	fEcrit(nIdFichier,sResultat,taille(sResultat))
      	SI ErreurDétectée alors
      		Erreur(<§$0009§>,ErreurInfo())
      		RENVOYER Faux
      	FIN
      	fferme(nIdFichier)
      
      	// On Affiche l'image
      	AfficherResultat()
      SINON
      	Erreur(<§$000a§>,ErreurInfo())
      	RENVOYER Faux
      FIN
      
      // Par défaut, la récupération se déroule correctement
      RENVOYER Vrai
     type : 458752
   -
     name : DernierResultat
     procedure_id : 2087280258189520120
     type_code : 14
     code : |1-
      Procédure DernierResultat()
      
      // On recherche le premier résultat
      gfnIndiceResultat = Dimension(gfnTableauResultat)
      
      // On regarde la dimension du tableau de résultat
      si Dimension(gfnTableauResultat)<gfnIndiceResultat ou gfnIndiceResultat =0 alors
      	Info(<§$000b§>)
      	RETOUR
      FIN
      
      // On va chercher l'image correspondante
      si bRecupererImage(gfnTableauResultat[gfnIndiceResultat])=Faux alors
      	// Erreur lors de la récupération de l'image
      	Info(<§$000c§>)
      	RETOUR
      FIN
      
      // On affiche le résultat
      AfficherResultat()
     type : 458752
   -
     name : ExecuterRecherche
     procedure_id : 2087280258189585921
     type_code : 14
     code : |1+
      Procédure ExecuterRecherche(bRechercheOU)
      
      tabResultatRecherche est un RechercherImagesResponse
      tabRes est un tableau de chaine
      nNbResultats est un entier
      nOccurence est un entier
      sMotsClés est une chaine
      
      // Appel au web service
      nOccurence = ListeOccurrence(LISTE_MotsClésARechercher)
      POUR i= 1 A nOccurence
      	sMotsClés += ["$"] + LISTE_MotsClésARechercher[i]
      FIN
      
      tabResultatRecherche = WD_Serveur_Webservice.RechercherImages(sMotsClés,bRechercheOU)
      
      // Traitement du résultat
      SI tabResultatRecherche.RechercherImagesResult..Occurrence > 0 ALORS
      
      	Pour i = 1 _A_ tabResultatRecherche.RechercherImagesResult..Occurrence
      		TableauAjoute(tabRes,tabResultatRecherche.RechercherImagesResult[i])
      	FIN
      	
      	// On mémorise le résultat
      	nNbResultats = nMemoriserResultat(tabRes)
      	
      	// On affiche le nombre d'image résultat
      	SELON nNbResultats
      		CAs 0 :
      			LIB_Resultat= <§$000f§>
      			Image_Resultat = ""
      			
      		CAS 1 :
      			LIB_Resultat= <§$0010§>
      		AUTRES CAS :
      			LIB_Resultat= ChaîneConstruit(<§$000d§>, nNbResultats)
      	FIN
      	
      	
      	// On affiche l'image correspondante
      	PremierResultat()
      	
      	// Si on a plus d'un résultat, on affiche les boutons de parcours
      	si nNbResultats>=1 alors
      		GROUPE_PARCOURS..Visible = Vrai
      	SINON
      		GROUPE_PARCOURS..Visible = Faux
      	FIN
      SINON
      	// Affichage de l'erreur
      	LIB_Resultat= <§$000f§>
      	Image_Resultat = ""
      	
      	Erreur(<§$000e§>)
      FIN
     type : 458752
   -
     name : nMemoriserResultat
     procedure_id : 2087280258189651753
     type_code : 14
     code : |1+
      Procédure nMemoriserResultat(tabResultatRecherche)
      
      nNbReponses est un entier
      
      // On libère l'ancien tableau des résultats
      gfnTableauResultat = allouer un tableau d'entier
      
      // On détermine le nombre de réponses 
      nNbReponses = TableauOccurrence(tabResultatRecherche)
      
      // On dimensionne le tableau à la taille nécessaire
      gfnTableauResultat = tabResultatRecherche
      
      // On renvoie le nombre de réponses
      RENVOYER nNbReponses
      
     type : 458752
   -
     name : PremierResultat
     procedure_id : 2087280258189717570
     type_code : 14
     code : |1-
      Procédure PremierResultat()
      
      // On recherche le premier résultat
      gfnIndiceResultat = 1
      
      // On regarde la dimension du tableau de résultat
      si Dimension(gfnTableauResultat)<gfnIndiceResultat alors
      	Info(<§$000b§>)
      	RETOUR
      FIN
      
      // On va chercher l'image correspondante
      si bRecupererImage(gfnTableauResultat[gfnIndiceResultat])=Faux alors
      	// Erreur lors de la récupération de l'image
      	Info(<§$000c§>)
      	RETOUR
      FIN
      
      // On affiche le résultat
      AfficherResultat()
     type : 458752
   -
     name : ResultatPrecedent
     procedure_id : 2087280262484750605
     type_code : 14
     code : |1-
      Procédure ResultatPrecedent()
      
      si gfnIndiceResultat <= 1 alors
      	Info(<§$0011§>)
      	PremierResultat()
      	RETOUR
      FIN
      
      // On recherche le premier résultat
      gfnIndiceResultat --
      
      // On regarde la dimension du tableau de résultat
      si Dimension(gfnTableauResultat)<gfnIndiceResultat alors
      	Info(<§$000b§>)
      	RETOUR
      FIN
      
      // On va chercher l'image correspondante
      si bRecupererImage(gfnTableauResultat[gfnIndiceResultat])=Faux alors
      	// Erreur lors de la récupération de l'image
      	Info(<§$000c§>)
      	RETOUR
      FIN
      
      // On affiche le résultat
      AfficherResultat()
     type : 458752
   -
     name : ResultatSuivant
     procedure_id : 2087280262484816437
     type_code : 14
     code : |1-
      Procédure ResultatSuivant()
      
      // On recherche le résultat suivant
      gfnIndiceResultat++
      
      // On regarde la dimension du tableau de résultat
      si Dimension(gfnTableauResultat)<gfnIndiceResultat alors	
      	si Dimension(gfnTableauResultat) <>0 alors 
      		Info(<§$0012§>)
      		DernierResultat()	
      	SINON
      		Info(<§$000b§>)	
      	FIN
      	RETOUR
      		
      FIN
      
      // On va chercher l'image correspondante
      si bRecupererImage(gfnTableauResultat[gfnIndiceResultat])=Faux alors
      	// Erreur lors de la récupération de l'image
      	Info(<§$000c§>)
      	RETOUR
      FIN
      
      // On affiche le résultat
      AfficherResultat()
     type : 458752
